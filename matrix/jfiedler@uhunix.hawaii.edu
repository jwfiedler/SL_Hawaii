    <script>

        function formatNumber(value) {
            const num = parseFloat(value);
            return isNaN(num) ? value : num.toFixed(2);
        }

        function loadCSV(url, containerId) {
            Papa.parse(url, {
                download: true,
                header: true,
                complete: function(results) {
                    const table = document.createElement('table');
                    table.classList.add('csv-table');
                    
                    // Create table header
                    const thead = table.createTHead();
                    const headerRow = thead.insertRow();
                    Object.keys(results.data[0]).forEach((header, index) => {
                        if (index > 0) { // Skip the first column (index)
                            const th = document.createElement('th');
                            th.textContent = header;
                            headerRow.appendChild(th);
                        }
                    });

                    // Create table body
                    const tbody = table.createTBody();
                    results.data.forEach(row => {
                        const tr = tbody.insertRow();
                        Object.values(row).forEach((cell, index) => {
                            if (index > 0) { // Skip the first column (index)
                                const td = document.createElement('td');
                                td.textContent = formatNumber(cell);
                                tr.appendChild(td);
                            }
                        });
                    });
                    document.getElementById(containerId).appendChild(table);
                }
            });
        }
        document.addEventListener('DOMContentLoaded', () => {
            loadCSV('../output/SL_magnitude_results.csv', 'csv-table-container');
            loadCSV('path_to_projections_csv.csv', 'csv-table-container-projections');
        });

    </script>

